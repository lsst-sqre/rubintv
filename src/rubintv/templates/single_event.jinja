{% extends "_camera.jinja" %}

{% block pagesubtitle %}
  {{ channel.title }}
{% endblock pagesubtitle %}

{% block undertitle %}
 {{ event.date }}
{% endblock undertitle %}

{% block content %}

  <div class="event-info">
    <h2>
      <span id="date">{{ event.day_obs }}</span> : Sequence
      <span id="seqNum">{{ event.seq_num }}</span>
    </h2>
    <nav class="prev-next-nav">

    </nav>
  </div>
      <img id="eventImage" src="data:image/{{ event.ext }};base64,{{ image_data }}">
      <div id="eventName" class="desc">{{ event.key }}</div>

{% endblock content %}
