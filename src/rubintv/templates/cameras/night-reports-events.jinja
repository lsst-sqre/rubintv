<h3 class="the-date" data-date="{{ date }}">Night reports for: <span class="data">{{ date }}</span></h3>
<div id="night-reports">
  <div class="dashboard">
  {% if dashboard_data %}
  <ul class="dashboard-list">
    {% for d in dashboard_data %}
      <li>
        <span class="key">{{ d }}</span>
        <span class="value">{{ dashboard_data[d] }}</span>
      </li>
    {% endfor %}
  </ul>
  {% endif %}
    <script type="application/json" id="dashboard-json">
      {{ dashboard_json|tojson }}
    </script>
  </div>
  {% for event in events %}
    <figure class="event">
      <img src="{{ event.url }}?t={{ event.timestamp }}">
      <figcaption>
        {{ event.name }}
      </figcaption>
    </figure>
  {% endfor %}
</div>
