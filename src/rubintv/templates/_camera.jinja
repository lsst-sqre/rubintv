{% extends "_layout.jinja" %}
{% block pagetitle %}
  <a href="{{
    url_for('camera',
    location_name=location.name,
    camera_name=camera.name)
  }}">
    {{ camera.title }}
  </a>
  {% block inline_status %}
  {% endblock inline_status %}
{% endblock pagetitle %}

{% block further %}
  {% if camera_json %}
      <script>
        window.APP_DATA.camera = {{ camera_json|tojson }}
        window.APP_DATA.date = "{{ date }}"
      </script>
  {% endif %}
  <div id="historicalbusy" data-historicalbusy="{{ historical_busy }}">
  </div>
{% endblock further %}
